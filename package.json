{
  "name": "react-share",
  "version": "3.0.0",
  "description": "Social media share buttons and share counts for React.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=5.0.0"
  },
  "files": [
    "lib/**/*",
    "es/**/*"
  ],
  "scripts": {
    "build": "npm run lint && npm run build:commonjs && npm run build:es",
    "build:commonjs": "rimraf ./lib && cross-env BABEL_ENV=commonjs babel ./src --out-dir ./lib",
    "build:es": "rimraf ./es && cross-env BABEL_ENV=es babel ./src --out-dir ./es",
    "build-demo": "rimraf ./docs && cross-env NODE_ENV=production webpack -p --config webpack.demo.config.js",
    "run-demos": "webpack-dev-server --hot --config webpack.demo.config.js --progress",
    "lint": "eslint --ext .jsx --ext .js ./src ./demo",
    "prepublishOnly": "npm run build",
    "start": "npm run run-demos"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "social",
    "media",
    "social-media",
    "share",
    "button",
    "count"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nygardk/react-share"
  },
  "author": {
    "name": "Klaus Nyg√•rd",
    "email": "klaus.nygard@gmail.com",
    "url": "http://klausnygard.fi"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.15.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "4.8.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "jsonp": "^0.2.1",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  }
}
